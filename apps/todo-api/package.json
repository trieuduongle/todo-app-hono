    "db:schema:up": "drizzle-kit up --config ./drizzle.config.ts"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2",
    "drizzle-kit": "0.22.7",
    "drizzle-orm": "0.31.2",
    "hono": "4.4.5",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20240605.0",
    "wrangler": "3.60.1"
  }
}