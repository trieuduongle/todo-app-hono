{
  "name": "todo-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "wrangler dev src/index.ts --port 3000",
    "build": "echo Not support right now!!!",
    "deploy": "wrangler deploy --minify src/index.ts",
